{
    "name": "Multi-tab",

    "description": "Open a given set of URLs in multiple tabs",

    "manifest_version": 2,

    "version": "0.1",

    "browser_action": {
        "default_icon": "multi-tab-16.png"
    },

    "icons": {
        "16": "multi-tab-16.png",
        "128": "multi-tab-128.png"
    },

    "permissions": ["tabs"],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "options_page": "options.html"
}
